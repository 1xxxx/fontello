.font-embedded-0 { font-family: 'fontelico'; }
.font-embedded-1 { font-family: 'entypo'; }
.font-embedded-2 { font-family: 'fontawesome'; }
.font-embedded-3 { font-family: 'iconic'; }
.font-embedded-4 { font-family: 'typicons'; }
.font-embedded-5 { font-family: 'modernpics'; }
.font-embedded-6 { font-family: 'meteocons'; }
.font-embedded-7 { font-family: 'maki'; }
.font-embedded-8 { font-family: 'zocial'; }
.font-embedded-9 { font-family: 'brandico'; }
.font-embedded-10 { font-family: 'websymbols'; }

@font-face {
  font-family: 'fontelico';
  src: url('<%= asset_path("fontelico.eot") %>');
  src: url('<%= asset_path("fontelico.eot") %>?#iefix') format('embedded-opentype'),
       url('<%= asset_path("fontelico.woff") %>') format('woff'),
       url('<%= asset_path("fontelico.ttf") %>') format('truetype'),
       url('<%= asset_path("fontelico.svg") %>#fontelico') format('svg');
  font-weight: normal;
  font-style: normal;
}

