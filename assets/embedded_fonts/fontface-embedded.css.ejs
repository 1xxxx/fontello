@font-face {
  font-family: 'fontelico';
  src: url('<%= asset_path("embedded_fonts/fontelico.eot") %>');
  src: url('<%= asset_path("embedded_fonts/fontelico.eot") %>?#iefix') format('embedded-opentype'),
       url('<%= asset_path("embedded_fonts/fontelico.woff") %>') format('woff'),
       url('<%= asset_path("embedded_fonts/fontelico.ttf") %>') format('truetype'),
       url('<%= asset_path("embedded_fonts/fontelico.svg") %>#fontelico') format('svg');
  font-weight: normal;
  font-style: normal;
}

