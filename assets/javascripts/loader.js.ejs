//= require fontello/vendor/modernizr.custom
//= require fontello/vendor/yepnope
//= require_self


/*jshint browser:true,node:false*/
/*global yepnope, N, window*/


yepnope([
  {
    //  JSON is required for Opera < 11 + IE < 8
    //  See: http://caniuse.com/json
    test: !!window.JSON,
    nope: <%- JSON.stringify(asset_path('fontello/vendor/json2.js')) %>
  },
  {
    load: <%- JSON.stringify(asset_path('fontello/javascripts/app.js')) %>,
    complete: function () {
      N.emit('init_complete');
    }
  }
]);


// vim: filetype=javascript:syntax=javascript
